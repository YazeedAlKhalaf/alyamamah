{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "alyamamah (local)",
      "request": "launch",
      "type": "dart",
      "args": ["--dart-define-from-file=dart-define/local.json"]
    },
    {
      "name": "alyamamah (prod)",
      "request": "launch",
      "type": "dart",
      "args": ["--dart-define-from-file=dart-define/prod.json"]
    },
    {
      "name": "alyamamah (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "alyamamah (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    },
    {
      "name": "Golden",
      "request": "launch",
      "type": "dart",
      "codeLens": {
        "for": ["run-test", "run-test-file"]
      },
      "args": ["--update-goldens"]
    },
    {
      "command": "./website/node_modules/.bin/astro dev",
      "name": "Development server",
      "request": "launch",
      "type": "node-terminal"
    }
  ]
}
